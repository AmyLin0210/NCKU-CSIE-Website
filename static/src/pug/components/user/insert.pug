mixin insert() 
  - 
    const i18n = Object.freeze( {
      [ LANG.getLanguageId( 'en-US' ) ]: {
        button: {
          check: 'ckeck',
          cancel: 'cancel',
        },
      },
      [ LANG.getLanguageId( 'zh-TW' ) ]: {
        button: {
          check: 'ckeck',
          cancel: 'cancel',
        },
      },
    } );
  section.insert.insert--hide
    section.insert__block
      article.block__title
        section.title__word= "變更您的姓名"
      article.block__input
        +image( {
          alt:       '>',
          className: `input__flag`,
          src:       `icon/tw.png`,
        } )
        input.input__content
      article.block__button
        button.button__item.button__item--cancel= i18n[ LANG.id ].button.cancel
        button.button__item.button__item--check=  i18n[ LANG.id ].button.check